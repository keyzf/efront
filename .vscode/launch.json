    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "efront2",
            "cwd": "${workspaceFolder}/../efront2/",
            "env": {
                "coms_path": "./coms",
                "public_path": "public",
                "page": "./coms/efront",
                "app": "efront/index.js",
                "extt": ".js",
                "coms": "./",
                "destpath": "public/efront.js"
            },
            "args": [
                "publish"
            ],
            "program": "${workspaceFolder}/main.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Slider",
            "env": {
                "app": "slider.js"
            },
            "args": [
                "build"
            ],
            "program": "${workspaceFolder}/main.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Reptile",
            "env": {
                "app": "reptile"
            },
            "args": [
                "m3u8Download_test"
            ],
            "program": "${workspaceFolder}/main.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "demo:Zimoli",
            "args": [
                "demo"
            ],
            "program": "${workspaceFolder}/main.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "docs",
            "args": [
                "docs"
            ],
            "program": "${workspaceFolder}/main.js"
        }
    ]
}